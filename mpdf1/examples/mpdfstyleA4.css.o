body{font-family:DejaVuSansCondensed,sans-serif;font-size:11pt}p{text-align:justify;margin-bottom:4pt;margin-top:0}hr{width:70%;height:1px;text-align:center;color:#999;margin-top:8pt;margin-bottom:8pt}a,h1,h2{color:#006}a,h1,h3{font-weight:400}a{font-style:normal;text-decoration:underline}pre{font-family:DejaVuSansMono,monospaced;font-size:9pt;margin-top:5pt;margin-bottom:5pt}h1,h3{font-size:26pt}h1,h2{font-family:DejaVuSansCondensed,sans-serif;page-break-after:avoid;margin-top:18pt;border-top:.075cm solid #000;border-bottom:.075cm solid #000}h2{font-weight:700;font-size:12pt;margin-top:6pt;border-top:.07cm solid #000;border-bottom:.07cm solid #000;text-transform:uppercase}.breadcrumb,h1,h2,h3{margin-bottom:6pt}h3{margin-top:0;border-top:0;border-bottom:0;color:#000}.infobox,h4{margin-top:10pt}h3,h4,h5,h6{font-family:DejaVuSansCondensed,sans-serif;page-break-after:avoid}h4{font-size:13pt;color:#9f2b1e;margin-bottom:7pt;font-variant:small-caps;margin-collapse:collapse}h5,h6{font-weight:700}h5{font-style:italic;margin-bottom:4pt;font-size:11pt;color:#004;margin-top:8pt}h6{font-size:9.5pt;color:#333;margin-top:6pt}.breadcrumb{text-align:right;font-size:8pt;font-family:DejaVuSerifCondensed,serif;color:#666;font-weight:700;font-style:normal}.infobox{background-color:#ddb;text-align:center;border:1px solid #800}.big{font-size:1.5em}.red{color:#800}.slanted{font-style:italic}